<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="MFDSettings" type="MFDisplay.Configuration.MFDConfigurationSection, MFDisplay"/>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <log4net>
    <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender" >
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger - %message%newline" />
      </layout>
    </appender>
    <root>
      <level value="NONE" />
      <appender-ref ref="ConsoleAppender" />
    </root>
  </log4net>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7" />
  </startup>
  <MFDSettings
      FilePath="TestData\"
      DefaultConfig="TestModule"
      xLMFDOffsetStart="101"
      xLMFDOffsetFinish="772"
      xRMFDOffsetStart="902"
      xRMFDOffsetFinish="1573"
      yOffsetStart="133"
      yOffsetFinish="764"
      width="850"
      height="720"
      top="615"
      lMfdLeft="3000"
      rMfdLeft="3825"
    >
    <Modules>
      <add moduleName="TestModule" displayName="Test Module" filename="LMFD.jpg">
        <Configurations>
          <add name="LMFD" opacity="1.0"/>
          <add name="RMFD" opacity="1.0" />
        </Configurations>
      </add>
    </Modules>
  </MFDSettings>
</configuration>